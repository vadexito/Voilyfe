msgid "'%value%' does not appear to be a valid date"
msgstr "'%value%' ne semble pas être une date valide"

#: application/translations/_XMLforPoedit/validators/date.xml:10
msgid "'%value%' does not fit the date format '%format%'"
msgstr "'%value%' ne respecte pas le format de date : '%format%'"

#: application/translations/_XMLforPoedit/validators/digits.xml:4
msgid "'%value%' must contain only digits"
msgstr "'%value%' ne doit contenir que des nombres"

#: application/translations/_XMLforPoedit/validators/emailAddress.xml:7
msgid ""
"'%value%' is no valid email address in the basic format local-part@hostname"
msgstr ""
"'%value%' n'est pas une adresse email valide avec une format de type partie-"
"locale@nomhote"

#: application/translations/_XMLforPoedit/validators/emailAddress.xml:10
msgid "'%value%' exceeds the allowed length"
msgstr "'%value%' dépasse la longueur autorisée"

#: application/translations/_XMLforPoedit/validators/float.xml:7
msgid "'%value%' does not appear to be a float"
msgstr "'%value%' ne semble pas être un nombre de type flottant"

