msgid "The two given tokens do not match"
msgstr "Les deux éléments ne correspondent pas"

#: application/translations/_XMLforPoedit/validators/identical.xml:7
msgid "No token was provided to match against"
msgstr "Pas de valeur apportée pour vérifier une concordance"

#: application/translations/_XMLforPoedit/validators/inArray.xml:4
msgid "'%value%' was not found in the haystack"
msgstr "'%value%' n'a pas été trouvée dans l'ensemble de valeurs"

#: application/translations/_XMLforPoedit/validators/int.xml:4
msgid "Invalid type given. String or integer expected"
msgstr "Type invalide fourni. Chaine de caractères ou nombre entier attendus"

