msgid "'%value%' does not appear to be an integer"
msgstr "'%value%' n'est pas un entier"

#: application/translations/_XMLforPoedit/validators/lessThan.xml:4
msgid "'%value%' is not less than '%max%'"
msgstr "'%value%' n'est pas inférieur au maximum '%max%'"

#: application/translations/_XMLforPoedit/validators/notEmpty.xml:4
msgid "Value is required and can't be empty"
msgstr "Ce champs ne peut être laissé sans une valeur."

#: application/translations/_XMLforPoedit/validators/notEmpty.xml:7
msgid "Invalid type given. String, integer, float, boolean or array expected"
msgstr ""
"Type invalide fourni. Une chaine de caractère, un nombre entier, un nombre "
"flottant, un booléen ou un tableau sont autorisés"

#: application/translations/_XMLforPoedit/validators/stringLength.xml:7
msgid "'%value%' is less than %min% characters long"
msgstr "'%value%' contient moins de %min% caractères"

#: application/translations/_XMLforPoedit/validators/stringLength.xml:10
msgid "'%value%' is more than %max% characters long"
msgstr "'%value%' contient plus de %max% caractères"

#: application/translations/_XMLforPoedit/validators/file/count.xml:4
msgid "Too many files, maximum '%max%' are allowed but '%count%' are given"
msgstr ""
"Nombre trop important de fichier. Maximum de '%max%' autorisés contre '%count"
"%' fournis"

#: application/translations/_XMLforPoedit/validators/file/count.xml:7
msgid "Too few files, minimum '%min%' are expected but '%count%' are given"
msgstr ""
"Nombre trop faible de fichier. Minimum de  '%min%' attendus contre '%count%' "
"fournis"

#: application/translations/_XMLforPoedit/validators/file/exists.xml:4
msgid "File '%value%' does not exist"
msgstr "Le fichier '%value%' n'existe pas"

#: application/translations/_XMLforPoedit/validators/file/notExists.xml:4
msgid "File '%value%' exists"
msgstr "Le fichier '%value%' existe"

#: application/translations/_XMLforPoedit/validators/file/size.xml:4
msgid ""
"Maximum allowed size for file '%value%' is '%max%' but '%size%' detected"
msgstr ""
"La taille maximum autorisée pour le fichier '%value%' est '%max%' mais une "
"taille de '%size%' a été détectée"

#: application/translations/_XMLforPoedit/validators/file/size.xml:7
msgid ""
"Minimum expected size for file '%value%' is '%min%' but '%size%' detected"
msgstr ""
"La taille minimale attendue pour le fichier '%value%' est de '%min%' mais "
"'%size%' a été détectée"

#: application/translations/_XMLforPoedit/validators/file/size.xml:10
msgid "File '%value%' is not readable or does not exist"
msgstr "Le fichier '%value%' n'est pas lisible ou n'existe pas"

